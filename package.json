{
  "name": "ide-vala",
  "main": "./lib/main",
  "version": "0.1.0",
  "description": "Vala language support for Atom-IDE",
  "repository": "https://github.com/naaando/ide-vala",
  "license": "GPL",
  "keywords": [
    "atom-ide",
    "vala",
    "lsp",
    "language-client"
  ],
  "enhancedScopes": [
    "source.vala"
  ],
  "configSchema": {
    "vlsPath": {
      "type": "string",
      "title": "Vala language server path",
      "default": "vala-language-server",
      "description": "Use custom Vala language server path"
    }
  },
  "engines": {
    "atom": ">=1.21.0"
  },
  "package-deps": [
    "language-vala-modern",
    "atom-ide-ui"
  ],
  "dependencies": {
    "atom-languageclient": "0.8.1",
    "atom-package-deps": "^4.6.0"
  },
  "consumedServices": {
    "linter-indie": {
      "versions": {
        "2.0.0": "consumeLinterV2"
      }
    }
  },
  "providedServices": {
    "autocomplete.provider": {
      "versions": {
        "2.0.0": "provideAutocomplete"
      }
    }
  }
}
